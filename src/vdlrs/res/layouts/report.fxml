<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="vdlrs.controllers.ReportController"
      prefHeight="400.0" prefWidth="600.0"
fx:id="vbRoot">
    <HBox spacing="30" alignment="CENTER">
        <padding>
            <Insets topRightBottomLeft="10" />
        </padding>
        <Button text="Registration Report" prefWidth="250" onAction="#reportHandler"/>
        <Button text="License Report" prefWidth="250" onAction="#reportHandler"/>
        <Button text="Transact Report" prefWidth="250" onAction="#reportHandler"/>
    </HBox>
</VBox>